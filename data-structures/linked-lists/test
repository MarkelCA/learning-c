
        /*printf("\npos: %d\n", count);*/
        if (count + 1 == pos)
        { // If the next element is the desired one
            temp = p;

            /*printf("data temp: %d\n", temp->data);*/
            /*printf("data next: %d\n", (temp->next)->data);*/

            p = new;
            p->next = temp;

            printf("data p: %d\n", p->data);
            p->data = 5555555;
            printf("data p: %d\n", p->data);
            puts("");
            return;
        }
        p = p->next;
        count++;
        --------------------------------------------------

        } 
        else if (count + 1 == pos)
        { // If the next element is the desired one
            temp = p;

            printf("data temp: %d\n", temp->data);
            printf("data next: %d\n", (temp->next)->data);

            p = new;
            p->next = temp;

            printf("data p: %d\n", p->data);
            p->data = 5555555;
            printf("data p: %d\n", p->data);
            puts("");
            return;
        }
